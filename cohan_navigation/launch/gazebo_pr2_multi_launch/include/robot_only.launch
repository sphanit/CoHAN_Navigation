<launch>
  <!-- Node start delay to avoid crashing of navigation node -->
  <arg name="node_start_delay" default="4.0" />
  <arg name="localize" default="fake"/>gazebo
  <arg name="num_agents" default="2"/>
  <arg name="robot_planner" default="cohan"/>

  <include file="$(find cohan_navigation)/launch/gazebo_pr2_multi_launch/include/agent_tracking.launch">
    <arg name="num_agents" value="$(arg num_agents)"/>
  </include>

  <!-- <node pkg="agent_path_prediction" type="predict_goal.py" name="agent_goal_predict" output="screen"/> -->

  <include file="$(find cohan_navigation)/launch/gazebo_pr2_multi_launch/include/move_base_nav.launch">
    <arg name="node_start_delay" value="$(arg node_start_delay)"/>
    <arg name="robot_planner" value="$(arg robot_planner)"/>
  </include>
  
</launch>
