<launch>
  <!-- Node start delay to avoid crashing of navigation node -->
  <arg name="node_start_delay" default="4.0" />
  <arg name="localize" default="fake"/>
  <arg name="num_agents" default="0"/>

  <include file="$(find cohan_navigation)/launch/webots_pr2_multi_launch/include/robo_description.launch"/>

  <include file="$(find cohan_navigation)/launch/webots_pr2_multi_launch/include/localization.launch">
    <arg name="localize" value="$(arg localize)"/>
  </include>

  <include file="$(find cohan_navigation)/launch/webots_pr2_multi_launch/include/move_base_nav_smb.launch">
    <arg name="node_start_delay" value="$(arg node_start_delay)"/>
  </include>



</launch>
